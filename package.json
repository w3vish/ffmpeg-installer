{
  "name": "@w3vish/ffmpeg-installer",
  "version": "1.0.0",
  "description": "Cross-platform FFmpeg and FFprobe binary installer",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "postinstall": "node dist/scripts/postinstall.js"
  },
  "keywords": [
    "ffmpeg",
    "ffprobe",
    "installer",
    "binary",
    "ffmpeg installer",
    "static ffmpeg",
    "cross-platform"
  ],
  "author": "w3vish",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/w3vish/ffmpeg-installer"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "dist",
    "binaries",
    "README.md",
    "LICENSE"
  ],
  "devDependencies": {
    "@types/node": "^20.17.24",
    "@types/tar": "^6.1.13",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "eslint": "^8.50.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "axios": "^1.8.4",
    "extract-zip": "^2.0.1",
    "tar": "^6.2.1"
  }
}
