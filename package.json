{
  "name": "@w3vish/ffmpeg-installer-demo",
  "version": "1.0.15",
  "type": "module",
  "description": "Cross-platform FFmpeg and FFprobe binary installer",
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "exports": {
    "import": "./dist/esm/index.js",
    "require": "./dist/cjs/index.cjs"
  },
  "scripts": {
    "build": "rm -rf dist && tsc --project tsconfig.esm.json && tsc --project tsconfig.cjs.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build"
  },
  "bin": {
    "ffmpeg-installer-demo": "./dist/cjs/installer.cjs"
  },
  "keywords": [
    "ffmpeg",
    "ffprobe",
    "installer",
    "binary",
    "ffmpeg installer",
    "static ffmpeg",
    "cross-platform"
  ],
  "author": "w3vish",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/w3vish/ffmpeg-installer"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "dist/cjs/installer.cjs"
  ],
  "devDependencies": {
    "@types/node": "^20.17.24",
    "extract-zip": "^2.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "axios": "^1.8.4",
    "readline": "^1.3.0"
  }
}
