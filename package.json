{
  "name": "@w3vish/ffmpeg-installer-demo",
  "version": "1.0.8",
  "type": "module",
  "description": "Cross-platform FFmpeg and FFprobe binary installer",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "bin": {
    "ffmpeg-installer-demo": "./dist/installer.js"
  },
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build",
    "download-binaries": "ts-node src/scripts/download-binaries.ts"
  },
  "keywords": [
    "ffmpeg",
    "ffprobe",
    "installer",
    "binary",
    "ffmpeg installer",
    "static ffmpeg",
    "cross-platform"
  ],
  "author": "w3vish",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/w3vish/ffmpeg-installer"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "dist/install.js"
  ],
  "devDependencies": {
    "@types/node": "^20.17.24",
    "extract-zip": "^2.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "axios": "^1.8.4",
    "date-fns": "^4.1.0",
    "readline": "^1.3.0"
  }
}
